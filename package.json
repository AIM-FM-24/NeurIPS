		"gh-pages": "^5.0.0",
		"svelte-icons": "^2.1.0",
		"svelte-preprocess": "^6.0.3",
		"svelte-youtube-embed": "^0.2.9",
		"tailwind-merge": "^1.14.0",
		"three": "^0.150.1"
	},
	"overrides": {
		"cookie": "^0.7.2"
	}
